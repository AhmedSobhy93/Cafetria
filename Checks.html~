 <script type="text/javascript">
		   
             $(document).ready(function()
             {
                 $('.RowToClick').click(function ()
                 {
                     $(this).nextAll('tr').each( function()
                     {
                         if ($(this).is('.RowToClick'))
                        {
                           return false;
                        }
                        $(this).toggle(350);
                     });
                 });
             });
 </script>

